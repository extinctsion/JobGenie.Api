{
  "Routes": [
    {
      "ServiceName": "UserApi",
      "UseServiceDiscovery": true,
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "Patch", "OPTIONS" ],
      "UpstreamPathTemplate": "/u/{everything}",
      "SwaggerKey": "UserApi"
    },
    {
      "ServiceName": "BadgeApi",
      "UseServiceDiscovery": true,
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "Patch", "OPTIONS" ],
      "UpstreamPathTemplate": "/b/{everything}",
      "SwaggerKey": "BadgeApi"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "UserApi",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "User API",
          "Version": "v1",
          "Url": "http://vectr-dev-user-cip-svc/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "BadgeApi",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Badge API",
          "Version": "v1",
          "Url": "http://vectr-dev-badge-cip-svc/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "SkipSslValidation": true,
    "AdministrationPath": "/administration",
    "ServiceDiscoveryProvider": {
      "Host": "vectr-dev-consul-cip-svc",
      "Port": 80,
      "Type": "PollConsul"
    }
  },
  "Logging": {
    "Level": "Trace"
  }
}