{
  "Routes": [
    {
      "ServiceName": "UserApi",
      "UseServiceDiscovery": true,
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "Patch", "OPTIONS" ],
      "UpstreamPathTemplate": "/u/{everything}",
      "SwaggerKey": "UserApi"
    },
    {
      "ServiceName": "ResumeApi",
      "UseServiceDiscovery": true,
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "Patch", "OPTIONS" ],
      "UpstreamPathTemplate": "/b/{everything}",
      "SwaggerKey": "ResumeApi"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "UserApi",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "User API",
          "Version": "v1",
          "Url": "http://user.api/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "ResumeApi",
      "TransformByOcelotConfig": true,ss
      "Config": [
        {
          "Name": "Resume API",
          "Version": "v1",
          "Url": "http://resume.api/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "SkipSslValidation": true,
    "AdministrationPath": "/administration",
    "ServiceDiscoveryProvider": {
      "Host": "cca-local-consul-svc",
      "Port": 8500,
      "Type": "PollConsul"
    }
  },
  "Logging": {
    "Level": "Trace"
  }
}